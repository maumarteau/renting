{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vue",
  "nodeVersion": "14.x",
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs14.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}